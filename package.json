{
  "name": "ember-loading",
  "version": "1.0.0",
  "description": "Primitives for handling loading state in Ember apps",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/kaliber5/ember-loading.git",
  "license": "MIT",
  "author": "Simon Ihmig <simon.ihmig@kaliber5.de>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:tsc": "tsc --noEmit",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-typescript": "^4.1.0",
    "ember-concurrency": "^2.0.3",
    "ember-concurrency-async": "^1.0.0",
    "ember-concurrency-ts": "^0.2.2"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.4.2",
    "@glimmer/component": "1.0.4",
    "@glimmer/tracking": "1.0.4",
    "@types/ember": "3.16.5",
    "@types/ember-qunit": "3.4.14",
    "@types/ember-test-helpers": "1.0.10",
    "@types/ember-testing-helpers": "0.0.4",
    "@types/ember__test-helpers": "1.7.5",
    "@types/qunit": "2.11.2",
    "@types/rsvp": "4.0.4",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "1.11.3",
    "ember-cli": "3.25.3",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-cli-typescript-blueprints": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-page-title": "6.2.2",
    "ember-qunit": "5.1.4",
    "ember-resolver": "8.0.2",
    "ember-source": "3.25.3",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "2.21.0",
    "ember-try": "1.4.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-ember": "10.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.1",
    "qunit": "2.15.0",
    "qunit-dom": "1.6.0",
    "release-it": "14.9.0",
    "release-it-lerna-changelog": "3.1.0",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
